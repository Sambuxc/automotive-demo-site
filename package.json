{
  "name": "automotive-demo-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/automotive-demo-site/",
    "lint": "vue-cli-service lint",
    "linter": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "dependencies": {
    "pinia": "^2.2.4",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "3.3.3",
    "sass": "^1.79.4",
    "sass-loader": "^16.0.2",
    "vite": "^5.4.8"
  }
}
